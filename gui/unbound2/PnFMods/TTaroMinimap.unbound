		# 	(class $FullsizeAbsolute)
		# 	(bind scaleX "regenRatio")

		# 	(style
		# 		(backgroundColor = "0xFFFFFFFF")
		# 		(backgroundSize = "fill")
		# 	)

		# 	(bind colorTransform "regenBarColorTransform")
		# )
		# HP
		(block
			(class $FullsizeAbsolute)
			(bind scaleX "healthRatio")

			(style
				(backgroundColor = "0xFFFFFFFF")
				(backgroundSize = "fill")
			)

			(bind colorTransform "markerColorTransform")
		)
	)
